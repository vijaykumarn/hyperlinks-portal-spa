# .env.production - UPDATED FOR DUAL BACKEND

# Auth Server Configuration
VITE_AUTH_SERVER_URL=https://auth.yourapp.com
VITE_RESOURCE_SERVER_URL=https://api.yourapp.com

# Google OAuth2 Configuration
VITE_GOOGLE_CLIENT_ID=your-production-google-client-id
VITE_GOOGLE_REDIRECT_URI=https://yourapp.com/auth/callback
VITE_ENABLE_GOOGLE_AUTH=true

# Session Configuration
VITE_SESSION_COOKIE_NAME=JSESSIONID
VITE_SESSION_SAME_SITE=strict

# Request Configuration
VITE_REQUEST_TIMEOUT=15000
VITE_RETRY_ATTEMPTS=2

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_PWA=true
VITE_ENABLE_SERVICE_WORKER=true

# Production Settings
VITE_LOG_LEVEL=error
VITE_ENABLE_DEV_TOOLS=false
VITE_HOT_RELOAD=false

# Security
VITE_CSRF_ENABLED=true
VITE_SECURE_COOKIES=true

# Frontend App Configuration
VITE_APP_BASE_URL=https://yourapp.com
VITE_APP_NAME=URL Shortener

# Third-party Services
VITE_SENTRY_DSN=YOUR_PRODUCTION_SENTRY_DSN