# .env.development - CORRECTED FOR OAUTH2

# Backend Server URLs
VITE_AUTH_SERVER_URL=http://localhost:8090
VITE_RESOURCE_SERVER_URL=http://localhost:8080

# Request Configuration
VITE_REQUEST_TIMEOUT=10000
VITE_RETRY_ATTEMPTS=3

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_PWA=false
VITE_ENABLE_SERVICE_WORKER=false

# Development Settings
VITE_LOG_LEVEL=debug
VITE_ENABLE_DEV_TOOLS=true
VITE_HOT_RELOAD=true

# Security
VITE_CSRF_ENABLED=true
VITE_SECURE_COOKIES=false

# Frontend App Configuration
VITE_APP_BASE_URL=http://localhost:3000
VITE_APP_NAME=URL Shortener

# Third-party Services (leave empty for development)
VITE_SENTRY_DSN=